  "selection_criteria": {
    "current_margin": 0.2,
    "description": "ESC continuous current rating must be >= motor_current * (1 + margin)",
    "cells_match": "Battery cell count must fall within ESC min-max cell range",
    "weight_limit": "ESC mass is included in the total W0 calculation",
    "efficiency_priority": "Higher efficiency preferred when multiple ESCs meet current requirements"
  },
  "formulas": {
    "hc4_constraint": "I_motor <= 0.8 * I_ESC_cont",
    "motor_current": "I_motor = P_elec / V_battery = (T * V_cruise / eta_motor) / V_battery",
    "thrust_required": "T = (T/W)_min * W0 * g = 0.45 * W0 * 9.81",
    "system_efficiency": "eta_total = eta_esc * eta_motor * eta_prop",
    "voltage_drop": "V_drop = I_motor * R_esc  (internal resistance loss)",
    "power_loss_esc": "P_loss = I_motor^2 * R_esc / 1000  (kW)"
  },
